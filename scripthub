local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local lp = Players.LocalPlayer
local playersList = Players:GetPlayers()
local numberOfPlayers = playersList
local CoreGui = game:FindService("CoreGui")
local Window = OrionLib:MakeWindow({Name = "MetaScript Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "MetaHub_Config", IntroText = "Loading Hub..."})

Gui_Version = "1.1.4"

_G.HeadSize = 20
getgenv().ScriptInput = ""

local FillColor = Color3.fromRGB(255,255,255)
local DepthMode = "AlwaysOnTop"
local FillTransparency = 0.5
local OutlineColor = Color3.fromRGB(255,255,255)
local OutlineTransparency = 0
local uis = game:GetService("UserInputService")
local xrayOn = false

function KeybindNotifyJ()
OrionLib:MakeNotification({
        Name = "MetaScript Hub",
        Content = "You need to press J to open or close this script",
        Image = "rbxassetid://4483345998",
        Time = 3
})
end

function bucu()
wait(.1)
loadstring(game:HttpGet("https://raw.githubusercontent.com/sirapobsriumang/Auto-Parry/main/Real"))()
wait(.2)
loadstring(game:HttpGet("https://raw.githubusercontent.com/sirapobsriumang/Auto-Spam-Parry/main/Bladeballnew"))()
end

