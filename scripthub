local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Meta Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "MetaHub", IntroText = "Loading Hub..."})
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local lp = Players.LocalPlayer
local getplayer = game:GetService("Players"):GetPlayers()

function LinkCopied()
OrionLib:MakeNotification({
        Name = "Meta Hub",
        Content = "The link has been copied",
        Image = "rbxassetid://7733764083",
        Time = 4
})
end

OrionLib:MakeNotification({
        Name = "Welcome to script:)",
        Content = "Please enjoy my script, my friends:)",
        Image = "rbxassetid://7734053697",
        Time = 3
})

