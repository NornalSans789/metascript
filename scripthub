                      print("Active!")
          end    
})

MainTab:AddLabel("For Loop Toggle")

MainTab:AddToggle({
	Name = "Loop WalkSpeed/JumpBoost",
	Default = false,
	Callback = function(loop)
getgenv().LoopAll = loop

if LoopAll == true then
while true do
wait(.1)
player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeed
player.Character.Humanoid.JumpPower = getgenv().JumpBoost
end
end
	end    
})

MainTab:AddToggle({
	Name = "Loop WalkSpeed",
	Default = false,
	Callback = function(loop)
getgenv().LoopWalkSpeed = loop

if LoopWalkSpeed == true then
while true do
wait(.1)
player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeed
end
end
	end    
})

MainTab:AddToggle({
	Name = "Loop JumpBoost",
	Default = false,
	Callback = function(loop)
getgenv().LoopJumpBoost = loop

if LoopJumpBoost == true then
while true do
wait(.1)
player.Character.Humanoid.JumpPower = getgenv().JumpBoost
end
end
	end    
})

local MainScriptSection = MainTab:AddSection({
        Name = "Toggle Main Section"
})

MainTab:AddToggle({
	Name = "Inf Jump",
	Default = false,
	Callback = function(jump)
getgenv().InfJ = jump
game:GetService("UserInputService").JumpRequest:connect(function()
    if InfJ == true then
        game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end)
	end    
})

MainTab:AddToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(noclip)
getgenv().Noclip = noclip
game:GetService("RunService").Heartbeat:Connect(function()
    if Noclip == true then 
       game:GetService("RunService").Stepped:wait()
       game.Players.LocalPlayer.Character.Head.CanCollide = false
       game.Players.LocalPlayer.Character.Torso.CanCollide = false
    else
       game:GetService("RunService").Stepped:wait()
       game.Players.LocalPlayer.Character.Head.CanCollide = true
       game.Players.LocalPlayer.Character.Torso.CanCollide = true
    end
end)
	end    
})

MainTab:AddSlider({
	Name = "Hitbox Size",
	Min = 20,
	Max = 500,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Size",
	Callback = function(size)
	_G.HeadSize = size
	end    
})

MainTab:AddToggle({
	Name = "Hitbox",
	Default = false,
	Callback = function(hit)
getgenv().Hitbox = hit

game:GetService('RunService').RenderStepped:connect(function()
if Hitbox == true then
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
while Hitbox == true do
wait(.1)
v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.HumanoidRootPart.Transparency = 0.7
v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really blue")
v.Character.HumanoidRootPart.Material = "Neon"
v.Character.HumanoidRootPart.CanCollide = false
end
while Hitbox == false do
_G.HeadSize = 1
v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.HumanoidRootPart.Transparency = 1
end
end
end
end
end)
	end    
})

