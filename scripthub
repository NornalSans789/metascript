        game.Workspace.Gravity = a
                print(a)
        end          
})

MainTab:AddLabel("For Button Reset")

MainTab:AddButton({
        Name = "Reset WalkSpeed",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
                      print("Active!")
          end    
})

MainTab:AddButton({
        Name = "Reset Jump Height",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
                      print("Active!")
          end    
})

MainTab:AddButton({
        Name = "Reset Gravity",
        Callback = function()
game.Workspace.Gravity = 155
                      print("Active!")
          end    
})

