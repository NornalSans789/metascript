MainTab:AddLabel("For Loop Toggle")

MainTab:AddToggle({
	Name = "Loop All",
	Default = false,
	Callback = function(loop)
getgenv().LoopAll = loop

if LoopAll == true then
while LoopAll == true then
wait(.1)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = getgenv().WalkSpeed
game.Players.LocalPlayer.Character.Humanoid.JumpPower = getgenv().JumpBoost
game.Workspace.Gravity = getgenv().Gravity
game.Workspace.CurrentCamera.FieldOfView = getgenv().Fov
end
end
	end    
})

