function CheckKeyNotify()
OrionLib:MakeNotification({ 
    Name = "Checking Key", 
    Content = "Checking The Key You Entered", 
    Image = "rbxassetid://4483345998", 
    Time = 3 
})
end

function ngulon()
OrionLib:MakeNotification({
        Name = "Logged In!",
        Content = "You need key "..Player.Name..".",
        Image = "rbxassetid://4483345998",
        Time = 3.5
})
end

function readKey()
     if readfile("/MetaScriptKey/key.txt") == loadedKey then
                  ConfigLoaded()
                  wait(1)
                  NotifyStartHub()
          else
                  ngulon()
                  local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = false, ConfigFolder = "MetaScriptKey", IntroText = "Key System..."}) 
                  local KeyTab = Window:MakeTab({
        Name = "Main",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

KeyTab:AddTextbox({
        Name = "Key",
        Default = "",
        TextDisappear = false,
        Callback = function(Value)
            getgenv().KeyInput = Value
        end          
})

KeyTab:AddButton({
        Name = "Check Key",
        Callback = function()
        if getgenv().KeyInput == loadedKey then 
            CheckKeyNotify()
            wait(2) 
            OrionLib:MakeNotification({ 
                Name = "Correct Key!", 
                Content = "The key you entered is Correct.", 
                Image = "rbxassetid://4483345998", 
                Time = 2 
            }) 
            wait(1) 
            OrionLib:Destroy() 
            wait(.2) 
            writefile("/MetaScriptKey/key.txt", loadedKey)
            NotifyStartHub()
       else 
           CheckKeyNotify()
           wait(2) 
           OrionLib:MakeNotification({ 
               Name = "Incorrect Key!", 
               Content = "The key you entered is incorrect.", 
               Image = "rbxassetid://4483345998", 
               Time = 3.5 
           }) 
       end
          end    
})

KeyTab:AddButton({
        Name = "Copy Key",
        Callback = function()
                      setclipboard("https://discord.gg/KKtc8PTKkz")
              OrionLib:MakeNotification({
        Name = "Key System",
        Content = "The key link has been copied",
        Image = "rbxassetid://4483345998",
        Time = 3.5
})
          end    
})
KeyTab:AddParagraph("Note:","This "Copy Key" button will copy our discord because in our discord there is a key")

local CreditTab = Window:MakeTab({
        Name = "Credit",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local CreditSection = CreditTab:AddSection({
        Name = "Script Helpers"
})

CreditTab:AddLabel("By SansYT789(Youtube)")
CreditTab:AddLabel("By RoyRedRedVN(Youtube)")

CreditTab:AddButton({
        Name = "Copy Discord",
        Callback = function()
                      setclipboard("https://discord.gg/KKtc8PTKkz")
              OrionLib:MakeNotification({
        Name = "MetaScript Hub",
        Content = "The link has been copied",
        Image = "rbxassetid://4483345998",
        Time = 4
})
          end    
})
          end
end


if isfolder("/MetaScriptKey") then
     if isfile("/MetaScriptKey/key.txt", "") then
         readKey()
     else
            writefile("/MetaScriptKey/key.txt", "")
            readKey()
     end
else
       makefolder("MetaScriptKey")
       writefile("/MetaScriptKey/key.txt", "")
       readKey()
end

OrionLib:Init()