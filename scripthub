MainTab:AddLabel("For Input")

MainTab:AddTextbox({
        Name = "WalkSpeed (Default = 16)",
        Default = "",
        TextDisappear = true,
        Callback = function(a)
        getgenv().WalkSpeed = a
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
                print(a)
        end          
})

MainTab:AddTextbox({
        Name = "Jump Height (Default = 50)",
        Default = "",
        TextDisappear = true,
        Callback = function(a)
        getgenv().JumpBoost = a
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
                print(a)
        end          
})

MainTab:AddTextbox({
        Name = "Gravity (Default = 150)",
        Default = "",
        TextDisappear = true,
        Callback = function(a)
        getgenv().Gravity = a
        game.Workspace.Gravity = a
                print(a)
        end          
})

MainTab:AddTextbox({
        Name = "Fov (Default = 70)",
        Default = "",
        TextDisappear = true,
        Callback = function(a)
        getgenv().Fov = a
        game.Workspace.CurrentCamera.FieldOfView = a
                print(a)
        end          
})

MainTab:AddLabel("For Reset Button")

MainTab:AddButton({
        Name = "Reset All",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
game.Workspace.Gravity = 150
game.Workspace.CurrentCamera.FieldOfView = 70
                      print("Active!")
          end    
})

MainTab:AddButton({
        Name = "Reset WalkSpeed",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
                      print("Active!")
          end    
})

MainTab:AddButton({
        Name = "Reset Jump Height",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
                      print("Active!")
          end    
