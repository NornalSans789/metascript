-- Local
local Player = game.Players.LocalPlayer
local loadedKey = loadstring(game:HttpGet("https://raw.githubusercontent.com/NornalSans789/key/main/skibidikey"))()
local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
getgenv().KeyInput = "string"

Gui_Version = "1.1.0"

-- function
function ConfigLoaded()
OrionLib:MakeNotification({
        Name = "Configuration Loaded!",
        Content = "Loaded the config key you previously entered",
        Image = "rbxassetid://4483345998",
        Time = 3
})
end

function NotifyStartHub()
wait(.5)
MakeScriptHub()
OrionLib:MakeNotification({
        Name = "Welcome to script:)",
        Content = "Please enjoy my script, my friends:)",
        Image = "rbxassetid://4483345998",
        Time = 3
})
ngudot()
end

function ngudot()
wait(1)
OrionLib:MakeNotification({
        Name = "Script Has Updated!",
        Content = "Enjoy my script now!!!!!",
        Image = "rbxassetid://4483345998",
        Time = 2.5
})
end

function MakeScriptHub()
local Window = OrionLib:MakeWindow({Name = "MetaScript Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "MetaScriptHub_Config", IntroText = "Loading Hub..."})

-- About Tab
local AboutTab = Window:MakeTab({
        Name = "About",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local AboutSection = AboutTab:AddSection({
        Name = "About Section"
})

AboutTab:AddLabel("Version: "..Gui_Version)

local ChangelogSection = AboutTab:AddSection({
        Name = "Changelogs Section"
})

AboutTab:AddLabel("[+] Added new Tab About for script")
AboutTab:AddLabel("[+] Added changelogs, you can see the changelog of this script!!!")
AboutTab:AddLabel("[+] Edited the notification bar to make it more reasonable")

