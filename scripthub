local MainTab = Window:MakeTab({
        Name = "Main",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local MainPlayerSection = MainTab:AddSection({
        Name = "Player Main Section"
})

MainTab:AddLabel("For Slider")

MainTab:AddSlider({
        Name = "WalkSpeed",
        Min = 16,
        Max = 1000,
        Default = 5,
        Color = Color3.fromRGB(255,255,255),
        Increment = 1,
        ValueName = "Speed",
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
                print(a)
        end    
})

MainTab:AddSlider({
        Name = "Jump Height",
        Min = 50,
        Max = 2000,
        Default = 5,
        Color = Color3.fromRGB(255,255,255),
        Increment = 1,
        ValueName = "Height",
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
                print(a)
        end    
})

MainTab:AddSlider({
        Name = "Gravity",
        Min = 155,
        Max = 3000,
        Default = 5,
        Color = Color3.fromRGB(255,255,255),
        Increment = 1,
        ValueName = "Heavy",
        Callback = function(a)
        game.Workspace.Gravity = a
                print(a)
        end    
})

MainTab:AddLabel("For Input")

MainTab:AddTextbox({
        Name = "WalkSpeed (Default = 16)",
        Default = "",
        TextDisappear = false,
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
                print(a)
        end          
})

MainTab:AddTextbox({
        Name = "Jump Height (Default = 50)",
        Default = "",
        TextDisappear = false,
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
                print(a)
        end          
})

MainTab:AddTextbox({
        Name = "Gravity (Default = 155)",
        Default = "",
        TextDisappear = false,
        Callback = function(a)
        game.Workspace.Gravity = a
                print(a)
        end          
})

MainTab:AddLabel("For Reset Button")

MainTab:AddButton({
        Name = "Reset WalkSpeed",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
                      print("Active!")
          end    
})

MainTab:AddButton({
        Name = "Reset Jump Height",
        Callback = function()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
                      print("Active!")
          end    
})

MainTab:AddButton({
        Name = "Reset Gravity",
        Callback = function()
game.Workspace.Gravity = 155
                      print("Active!")
          end    
})

local MainScriptSection = MainTab:AddSection({
        Name = "Script Main Section"
})

MainTab:AddButton({
        Name = "Inf Jump (Forever)",
        Callback = function()
local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
    if InfiniteJumpEnabled then
        game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end
end)
          end    
})

