local MainPlayerSection = MainTab:AddSection({
        Name = "Main Player Section"
})

MainTab:AddLabel("For Slider")

MainTab:AddSlider({
        Name = "WalkSpeed",
        Min = 16,
        Max = 1000,
        Default = 5,
        Color = Color3.fromRGB(255,255,255),
        Increment = 1,
        ValueName = "Speed",
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
                print(a)
        end    
})

MainTab:AddSlider({
        Name = "Jump Height",
        Min = 50,
        Max = 2000,
        Default = 5,
        Color = Color3.fromRGB(255,255,255),
        Increment = 1,
        ValueName = "Height",
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
                print(a)
        end    
})

MainTab:AddSlider({
        Name = "Gravity",
        Min = 155,
        Max = 3000,
        Default = 5,
        Color = Color3.fromRGB(255,255,255),
        Increment = 1,
        ValueName = "Heavy",
        Callback = function(a)
        game.Workspace.Gravity = a
                print(a)
        end    
})

MainTab:AddLabel("For Input")

MainTab:AddTextbox({
        Name = "WalkSpeed (Default = 16)",
        Default = "",
        TextDisappear = true,
        Callback = function(a)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
                print(a)
        end          
})

