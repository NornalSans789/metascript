local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "MetaScript Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "MetaHub_Config", IntroText = "Loading Hub..."})

Gui_Version = "1.1.2"

function KeybindNotifyJ()
OrionLib:MakeNotification({
        Name = "MetaScript Hub",
        Content = "You need to press J to open or close this script",
        Image = "rbxassetid://4483345998",
        Time = 3
})
end

function bucu()
wait(.1)
loadstring(game:HttpGet("https://raw.githubusercontent.com/sirapobsriumang/Auto-Parry/main/Real"))()
wait(.2)
loadstring(game:HttpGet("https://raw.githubusercontent.com/sirapobsriumang/Auto-Spam-Parry/main/Bladeballnew"))()
end

function LinkCopied()
OrionLib:MakeNotification({
        Name = "MetaScript Hub",
        Content = "The link has been copied",
        Image = "rbxassetid://4483345998",
        Time = 4
})
end

